<h1>Categories:<h1>
<div class="large-12 small-12 columns">
<div class="row">

  <%= link_to likes_path(cat: 'Tops') do %>
    <div class="large-2 columns">
      <div class="panel">
        <h5>Tops</h5>
      </div>
    </div>
  <% end %>

  <%= link_to likes_path(cat: "Bottoms") do %>
    <div class="large-2 columns">
      <div class="panel">
        <h5>Bottoms</h5>
      </div>
    </div>
  <% end %>

  <%= link_to likes_path(cat: "Dresses") do %>
    <div class="large-2 columns">
      <div class="panel">
        <h5>Dresses</h5>
      </div>
    </div>
  <% end %>

  <%= link_to likes_path(cat: "Outerwear") do %>
    <div class="large-2 columns">
      <div class="panel">
        <h5>Outerwear</h5>
      </div>
    </div>
  <% end %>

  <%= link_to likes_path(cat: "Accessories") do %>
    <div class="large-2 columns">
      <div class="panel">
        <h5>Accessories</h5>
      </div>
    </div>
  <% end %>

  <%= link_to likes_path(cat: "Shoes") do %>
    <div class="large-2 columns">
      <div class="panel">
        <h5>Shoes</h5>
      </div>
    </div>
  <% end %>
  </div>
</div>



<div class="row">
  <% @items.each do |item| %>
    <div class="large-4 columns">
      <ul class="pricing-table test">
        <li class="title product-name"><%= item.name %></li>
        <li class="price uniform"><%= link_to image_tag(item.image_url), item.click_url %></li>
        <li class="bullet-item">$<%= item.price %></li>
        <li class="cta-button"><a class="button" href="#">Buy Now</a></li>
      </ul>
    </div>
  <% end %>
</div>

      
 
    